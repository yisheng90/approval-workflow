{
  "name": "@shelf/jest-mongodb",
  "version": "1.1.3",
  "description": "Run your tests using Jest & MongoDB in Memory server",
  "keywords": [
    "jest",
    "jest environment",
    "jest preset",
    "mongodb",
    "mongodb local"
  ],
  "repository": "shelfio/jest-mongodb",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "scripts": {
    "lint": "eslint . --fix --ext .js,.json,.ts --quiet",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "./jest-preset.js"
  },
  "dependencies": {
    "cwd": "0.10.0",
    "debug": "4.1.1",
    "mongodb-memory-server": "6.0.1"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.8.2",
    "eslint": "6.5.1",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "mongodb": "3.3.3",
    "prettier": "1.18.2"
  },
  "peerDependencies": {
    "mongodb": "3.x.x"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
